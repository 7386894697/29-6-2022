


std={}
n=int(input('enter no of employee:'))
for i  in range(1,n+1):
    l=[]
    name=input('enter name of the employee:')
    l.append(name)
    designation=input('enter designation of th employee:')
    l.append(designation)
    empsalary=int(input('enter salary of the employee:'))
    l.append(empsalary)
    empphno=int(input('enter employee phno:'))
    l.append(empphno)
    std[i]=l
    print(std)
    
    
    
    
